{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "jarvis-rt"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
        "../node_modules/nouislider/distribute/nouislider.min.css",
        "../src/app/_shared/styles/nouislider-custom.css",
        "../public/icons/nucleo/css/style.css",
        "../node_modules/intro.js/minified/introjs.min.css",
        "../node_modules/ng2-tree/styles.css",
        "../src/app/_shared/styles/d3bomviewer-custom.css",
        "../src/app/_shared/styles/typeahead-custom.css",
        "../node_modules/simplebar/dist/simplebar.css",
        "../src/app/_shared/styles/simplebar-custom.css",
        "../node_modules/angular-tree-component/dist/angular-tree-component.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
        "../node_modules/fuse.js/dist/fuse.js",
        "../node_modules/chart.js/dist/chart.bundle.js",
        "../node_modules/intro.js/minified/intro.min.js",
        "../node_modules/typeahead.js/dist/typeahead.bundle.min.js",
        "../node_modules/typeahead.js/dist/bloodhound.min.js",
        "../node_modules/simplebar/dist/simplebar.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
